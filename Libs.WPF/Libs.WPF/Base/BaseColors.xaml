<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="ForegroundColor">White</SolidColorBrush>
    <SolidColorBrush x:Key="InverseForegroundColor">Black</SolidColorBrush>
    <SolidColorBrush x:Key="HighlightColor">#FCD535</SolidColorBrush>
    <SolidColorBrush x:Key="MouseOverButtonColor">#FFA955</SolidColorBrush>
    <SolidColorBrush x:Key="PressedButtonColor">#EA7300</SolidColorBrush>
    <SolidColorBrush x:Key="BackgroundColor">#212529</SolidColorBrush>
    <SolidColorBrush x:Key="SubBackgroundColor">#9EC6F3</SolidColorBrush>

    <SolidColorBrush x:Key="InputBackgroundColor">#1A1A1A</SolidColorBrush>
    <SolidColorBrush x:Key="InputBorderColor">#0A4B8C</SolidColorBrush>
    <SolidColorBrush x:Key="MouseOverItemBorderColor">#26A0DA</SolidColorBrush>

    <SolidColorBrush x:Key="DisableColor">#9A9C9E</SolidColorBrush>
    <SolidColorBrush x:Key="DisableBackgroundColor">LightGray</SolidColorBrush>

</ResourceDictionary>